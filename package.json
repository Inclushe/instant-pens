{
  "name": "instant-pens",
  "version": "0.0.1",
  "description": "Quickly gets a new or existing front-end project up-and-running.",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "pen": "./index.js"
  },
  "dependencies": {
    "browser-sync": "^2.24.5",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "commander": "^2.15.1"
  },
  "config": {
    "supportedPackages": {
      "pug": {
        "preprocessorType": "html",
        "fileTypes": [
          "pug",
          "jade"
        ]
      },
      "stylus": {
        "preprocessorType": "css",
        "fileTypes": [
          "styl",
          "stylus"
        ]
      }
    }
  }
}
